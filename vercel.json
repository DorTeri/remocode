{
    "builds": [
      { "src": "frontend/package.json", "use": "@now/static-build" },
      { "src": "backend/package.json", "use": "@now/node" }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/frontend/$1"
      }
    ]
  }
  